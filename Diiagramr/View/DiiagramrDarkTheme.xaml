<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Diiagramr color skin file. -->

    <Color x:Key="DarkGray" >#2D2D30</Color>
    <Color x:Key="SelectionBlue" >#3E8FD6</Color>
    <Color x:Key="DiiagramrBlack" >#101010</Color>
    <Color x:Key="DiiagramrOffWhite" >#E6E6E6</Color>

    <SolidColorBrush x:Key="ToolbarBackgroundBrush" Color="{StaticResource ResourceKey=DarkGray}"/>
    <SolidColorBrush x:Key="NodeSelectionStyleBrush" Color="{StaticResource ResourceKey=SelectionBlue}"/>
    <SolidColorBrush x:Key="DiagramBackgroundBrush" Color="{StaticResource ResourceKey=DiiagramrBlack}"/>
    <SolidColorBrush x:Key="DiagramSelectionRectangleStroke" Color="{StaticResource ResourceKey=DiiagramrOffWhite}"/>
</ResourceDictionary>